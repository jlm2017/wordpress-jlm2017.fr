{
  "name": "lafranceinsoumise/bedrock",
  "type": "project",
  "license": "AGPL",
  "homepage": "https://github.com/lafranceinsoumise/bedrock-lafranceinsoumise.fr",
  "authors": [
    {
      "name": "Jill Royer",
      "email": "hello@jillroyer.me",
      "homepage": "https://github.com/jillro"
    }
  ],
  "config": {
    "preferred-install": "dist"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "vcs",
      "url": "git@github.com:lafranceinsoumise/theme-wp"
    },
    {
      "type": "vcs",
      "url": "git@github.com:lafranceinsoumise/wp-plugin"
    },
    {
      "type": "vcs",
      "url": "git@github.com:lafranceinsoumise/wp-author-category"
    }
  ],
  "require": {
    "php": ">=5.6",
    "composer/installers": "^1.4.0",
    "vlucas/phpdotenv": "^2.0.1",
    "roots/wordpress": "^5.1",
    "oscarotero/env": "^1.0",
    "roots/wp-password-bcrypt": "1.0.0",
    "jlm2017/theme-wp": "dev-master",
    "fi/fi-wordpress-addons": "dev-master",
    "fi/author-category": "dev-master",
    "wpackagist-plugin/wordpress-seo": "^15.0",
    "wpackagist-plugin/redirection": "^4.0",
    "wpackagist-plugin/contact-form-7": "^5.0",
    "wpackagist-plugin/wpcf7-recaptcha": "^1.0",
    "wpackagist-plugin/elementor": "^3.0",
    "wpackagist-plugin/wp-maintenance": "^5.0",
    "wpackagist-plugin/google-authenticator": "^0",
    "wpackagist-plugin/members": "^3.0",
    "wpackagist-plugin/gregs-comment-length-limiter": "^1.7",
    "wpackagist-plugin/amp": "^2.0",
    "wpackagist-plugin/menu-export": "1.0.1",
    "wpackagist-plugin/autoptimize": "^2.5",
    "wpackagist-plugin/wp-typography": "^5.6",
    "wpackagist-plugin/pdfjs-viewer-shortcode": "^1.5.4",
    "wpackagist-plugin/redis-cache": "^2.0",
    "wpackagist-plugin/read-meter": "^1.0",
    "wpackagist-plugin/webp-simple": "^1.0",
    "wpackagist-theme/generatepress": "^3.0"
  },
  "extra": {
    "installer-paths": {
      "web/app/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "web/app/plugins/{$name}/": ["type:wordpress-plugin"],
      "web/app/themes/{$name}/": ["type:wordpress-theme"]
    },
    "wordpress-install-dir": "web/wp"
  }
}
